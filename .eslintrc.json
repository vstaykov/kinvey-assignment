{
  "root": true,
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "node": true
  },
  "rules": {
      "prettier/prettier": "error",
      "no-underscore-dangle": ["error", { "allowAfterThis": true }],
      "func-names": 0,
      "no-console": 0
  }
}
